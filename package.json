{
  "name": "attlas-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "next dev -p 3000",
    "dev2": "NODE_ENV='development' node server.js",
    "devwin": "set NODE_ENV='development' && node server.js",
    "build": "yarn clean && next build",
    "start": "next start",
    "export": "next export",
    "deploy": "pm2 deploy production",
    "lint": "eslint './src/**/*.js' --max-warnings 0 --fix",
    "build:docker-compose": "docker-compose build",
    "start:docker-compose": "docker-compose up -d",
    "build:docker": "docker build -t web-image .",
    "start:docker": "docker run --name web-container -p 3000:3000 -d web-image",
    "prod": "pm2 -v && pm2 start --no-daemon ecosystem.config.js",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install",
    "postbuild": "next-sitemap --config attlas-sitemap.js"
  },
